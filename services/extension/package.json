{
  "name": "security-seal",
  "displayName": "Security Seal",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "security-seal.authenticate",
        "title": "Authenticate with Security Seal"
      },
      {
        "command": "security-seal.analyze_code",
        "title": "Analyze code with Security Seal"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "security-seal.analyze_code",
          "when": "editorHasSelection"
        }
      ],
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "security-seal.analyze_code",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "build": "tsc && npm run copy-js",
    "copy-js": "cp -r js/* out/",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "compile-tests": "tsc -b --clean && tsc -b",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/sinon": "^17.0.3",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "sinon": "^17.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "axios": "^1.6.7",
    "dotenv": "^16.4.5"
  }
}
